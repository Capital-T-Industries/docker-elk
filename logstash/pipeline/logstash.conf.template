input {
	tcp {
		port => 5000
		codec => json
	}
}

filter {
   date {
        match => ["timestamp" , "ISO8601"]
        target => "@timestamp"
        add_field => { "debug" => "timestampMatched"}
   }
}

output {
	elasticsearch {
		hosts => "${ES_HOST}:${ES_PORT}"
		index => "sensor"
		## hosts => "167.99.106.248:9200"
	}
}
